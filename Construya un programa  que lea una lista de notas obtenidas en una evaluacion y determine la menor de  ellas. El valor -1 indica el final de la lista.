import javax.swing.*;
public class lista_detener {
    public static void main(String[] args) {
        double nota,min;
        min=21;
        nota=Double.parseDouble(JOptionPane.showInputDialog("Ingrese las notas"));
        while (nota!=-1) {            
            if (nota>=0 && nota<=20) {
                if (nota<min) {
                    min=nota;
                }
            }
            nota=Double.parseDouble(JOptionPane.showInputDialog("Ingrese las notas"));
        }
        JOptionPane.showMessageDialog(null, "La nota minima es: "+min);
    } 
}
